    <display-name>RecursionDir</display-name>
    <servlet-name>RecursionDir</servlet-name>
    <servlet-class>com.stbyte.tech.RecursionDir</servlet-class>
    <init-param>
      <description>从根目录开始递归</description>
      <param-name>ROOT</param-name>
      <param-value>/</param-value>
