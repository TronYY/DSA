  </servlet>
  <servlet-mapping>
    <servlet-name>RecursionDir</servlet-name>
    <url-pattern>/RD</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AndroidServlet</display-name>
    <servlet-name>AndroidServlet</servlet-name>
    <servlet-class>com.stbyte.servlet.AndroidServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AndroidServlet</servlet-name>
    <url-pattern>/AS</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DisplayDate</display-name>
    <servlet-name>DisplayDate</servlet-name>
    <servlet-class>com.stbyte.tech.DisplayDate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayDate</servlet-name>
    <url-pattern>/DisplayDate</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>HelloServlet</display-name>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.stbyte.tech.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DispatcherServlet</display-name>
    <servlet-name>DispatcherServlet</servlet-name>
    <servlet-class>com.stbyte.tech.DispatcherServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DispatcherServlet</servlet-name>
    <url-pattern>/SelectBeer.do</url-pattern>
  </servlet-mapping>
</web-app>